---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Tuxicity</title>
  <link href="http://tuxicity.se/atom.xml" rel="self"/>
  <link href="http://tuxicity.se/"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://tuxicity.se/</id>
  <author>
    <name>Johan Andersson</name>
    <email>johan.rejeep@gmail.com</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="http://tuxicity.se{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>http://tuxicity.se{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
</feed>
